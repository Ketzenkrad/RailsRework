<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>User</th>
      <th>Visible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
    <%= render partial: 'post', collection: @posts %>
    <% end %>
  </tbody>
</table>

<br>

<p class="pagination">
  <%= paginate @posts %>
</p>

<%= link_to 'New Post', new_post_path %>
